<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>StudyFormServlet</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>nz.aut.se2015.nsa.servlets.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EvidenceItemFormServlet</servlet-name>
        <servlet-class>nz.aut.se2015.nsa.servlets.EvidenceItemFormServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MethodFormServlet</servlet-name>
        <servlet-class>nz.aut.se2015.nsa.servlets.MethodFormServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudyFormServlet</servlet-name>
        <servlet-class>nz.aut.se2015.nsa.servlets.StudyFormServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitStudyServlet</servlet-name>
        <servlet-class>nz.aut.se2015.nsa.servlets.SubmitStudyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/IndexServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EvidenceItemFormServlet</servlet-name>
        <url-pattern>/EvidenceItemFormServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MethodFormServlet</servlet-name>
        <url-pattern>/MethodFormServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudyFormServlet</servlet-name>
        <url-pattern>/StudyFormServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitStudyServlet</servlet-name>
        <url-pattern>/SubmitStudyServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>